{
  "name": "netlify-plugin-ipx",
  "version": "0.0.0-14",
  "description": "on-demand image optimazation for Netlify",
  "license": "MIT",
  "exports": {
    "./function": {
      "import": "./dist/function.mjs",
      "require": "./dist/function.cjs"
    },
    ".": {
